{
  "name": "eco-event-tracker-api",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript backend foundation with models and SQL migrations",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "db:migrate:up": "psql \"$DATABASE_URL\" -f migrations/001_init_up.sql",
    "db:migrate:down": "psql \"$DATABASE_URL\" -f migrations/001_init_down.sql"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "typescript": "^5.6.3"
  }
}
